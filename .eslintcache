;\n  let sortedGroupKeys: string[] | undefined;\n\n  if (events.length) {\n    groupedEvents = groupEventsByDay(events);\n    sortedGroupKeys = Object.keys(groupedEvents).sort(\n      (date1, date2) => +new Date(date1) - +new Date(date2)\n    );\n  }\n\n  return groupedEvents && sortedGroupKeys ? (\n    <div className=\"calendar\">\n      {sortedGroupKeys.map((dayKey) => {\n        const events = groupedEvents ? groupedEvents[dayKey] : [];\n        const groupDate = new Date(dayKey);\n        const day = groupDate.getDate();\n        const month = groupDate.toLocaleString(undefined, { month: \"long\" });\n\n        return (\n          <div className=\"calendar-day\">\n            <div className=\"calendar-day-label\">\n              <span>\n                {day} {month}\n              </span>\n            </div>\n            <div className=\"calendar-events\">\n              {events.map((event) => {\n                return (\n                  <div className=\"calendar-event\">\n                    <div className=\"calendar-event-info\">\n                      <div className=\"calendar-event-time\">10:00 - 12:00</div>\n                      <div className=\"calendar-event-title\">{event.title}</div>\n                    </div>\n                    <button className=\"calendar-event-delete-button\">\n                      &times;\n                    </button>\n                  </div>\n                );\n              })}\n            </div>\n          </div>\n        );\n      })}\n    </div>\n  ) : (\n    <p>Loading...</p>\n  );\n};\n\nexport default connector(Calendar);\n","/home/hamza/Desktop/bootcamp_2020/record-app/src/lib/utils.ts",[],"/home/hamza/Desktop/bootcamp_2020/record-app/src/components/Calendar/index.ts",[],"/home/hamza/Desktop/bootcamp_2020/record-app/src/components/Calendar/Calendar.tsx",["60"],{"ruleId":"61","replacedBy":"62"},{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","severity":1,"message":"66","line":1,"column":10,"nodeType":"67","messag